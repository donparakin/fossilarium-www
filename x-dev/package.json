{
  "name": "fossilarium-web",
  "version": "1.0.0",
  "description": "www.fossilarium.com website",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "hugo:version": "cd ../hugo && hugo version",
    "hugo:build": "cd ../hugo && hugo --cleanDestinationDir",
    "hugo:serve": "cd ../hugo && hugo server",
    "hugo:serve+drafts": "cd ../hugo && hugo server --buildDrafts",
    "hugo:new-post": "cd ../hugo && hugo",
    "gulp:version": "cd gulp && gulp --version",
    "gulp:build": "cd gulp && gulp build",
    "gulp:watch": "cd gulp && gulp watch",
    "svelte:build": "rollup -c",
    "svelte:autobuild": "rollup -c -w",
    "svelte:dev": "run-p svelte:autobuild svelte:start-dev",
    "svelte:start": "sirv public --single",
    "svelte:start-dev": "sirv public --single --dev"
  },
  "author": "Don Parakin",
  "license": "UNLICENSED",
  "dependencies": {
    "hugo-bin": "*",
    "bulma": "*",
    "gulp": "*",
    "gulp-cli": "*",
    "gulp-concat": "*",
    "gulp-filter": "*",
    "gulp-sass": "*",
    "gulp-autoprefixer": "*",
    "gulp-uglify": "*",
    "gulp-sourcemaps": "*",
    "gulp-rename": "*",
    "pump": "*",
    "gulp-tar": "*",
    "gulp-gzip": "*",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "svelte": "^3.0.0",
    "sirv-cli": "^0.4.4"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/barenakedcoder/fossilarium-web.git"
  }
}
